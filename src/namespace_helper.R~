###Find the packages for each function in all functions

####################
rm(list=ls())
####################

library(bipartite)
library(mvbutils)

###enaR -> network
sapply(dir('~/projects/packages/enaR/R',full.name=TRUE),source)
sapply(dir('~/Downloads/network/R/',full.name=TRUE),source)
network.fl <- as.character(sapply(dir('~/Downloads/network/R/'),function(x) strsplit(x,'.R')[[1]][1]))
enar.fl <- as.character(sapply(dir('~/projects/packages/enaR/R'),function(x) strsplit(x,'.R')[[1]][1]))
fw <- foodweb()
fw.mat <- fw$funmat[fw.l%in%enar.fl,fw.l%in%enar.fl==FALSE]
plotweb(fw.mat)
mtext('enaR -> network')

####################
rm(list=ls())
####################
quartz()

###enaR -> sna
sapply(dir('~/projects/packages/enaR/R',full.name=TRUE),source)
sapply(dir('~/Downloads/sna/R/',full.name=TRUE),source)
sna.fl <- as.character(sapply(dir('~/Downloads/sna/R/'),function(x) strsplit(x,'.R')[[1]][1]))
enar.fl <- as.character(sapply(dir('~/projects/packages/enaR/R'),function(x) strsplit(x,'.R')[[1]][1]))
fw <- foodweb()
fw.l <- rownames(fw$funmat)
fw.mat <- fw$funmat[fw.l%in%enar.fl,fw.l%in%enar.fl==FALSE]
plotweb(fw.mat)
mtext('enaR -> sna')
